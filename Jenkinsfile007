pipeline{
    agent any
    stages{
        stage("git pull"){
            steps{
                git credentialsId: 'Jenkinsfile007', url: 'https://github.com/Mahindranampally/myweb.git'
            }
        }
        stage("build"){
            steps{
                sh "mvn clean package"
            }
        }
        stage("docker"){
            steps{
                sh "docker build -t mahindraid/mahiapp:0.0.1" .
            }
        
        }
    }
}
